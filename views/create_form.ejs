<%- include('layouts/header'); -%>
<div class="starter-template">
        <h1>Form List</h1>
        <br /><br />
<form method="POST" action="/create_form" >
  <div class="form-group">
    <label for="title">Form Title</label>
    <input type="text" name="title" class="form-control" id="title" aria-describedby="titleHelp" placeholder="Enter Title">
    <small id="titleHelp" class="form-text text-muted">Enter a title for this form</small>
  </div>
  <div class="form-group form-check">
    <input type="checkbox" name="enabled" class="form-check-input" id="is-active">
    <label class="form-check-label" for="is-active">Enabled</label>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
<%- include('layouts/footer'); -%>